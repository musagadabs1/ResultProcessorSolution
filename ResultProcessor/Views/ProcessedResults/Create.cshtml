@model ResultProcessor.Models.ProcessedResult

@{
    ViewData["Title"] = "Create";
}

<h2>Processe Result</h2>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            @*<div class="form-group">
                <div>ViewBag.Error</div>
            </div>*@
            <div class="form-group">
                <label asp-for="Semester" class="control-label"></label>
                <select asp-for="Semester" asp-items="(List<SelectListItem>)ViewBag.Semester" class="form-control"></select>
                <span asp-validation-for="Semester" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Session" class="control-label"></label>
                <select asp-for="Session" asp-items="(List<SelectListItem>)ViewBag.Sessions" class="form-control"></select>
                <span asp-validation-for="Session" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Level" class="control-label"></label>
                <select asp-for="Level" asp-items="(List<SelectListItem>)ViewBag.Level" class="form-control"></select>
                <span asp-validation-for="Level" class="text-danger"></span>
            </div>
            
            <div class="form-group">
                <input type="submit" value="Process" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

